{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to QEMU",
            "type": "cppdbg",
            "request": "attach",
            "program": "/path/to/vmlinux",
            "MIMode": "gdb",
            "miDebuggerServerAddress": "localhost:1234",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}